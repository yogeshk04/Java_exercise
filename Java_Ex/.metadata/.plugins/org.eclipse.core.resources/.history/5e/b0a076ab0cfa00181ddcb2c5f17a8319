package com.course.exercise;

import java.util.Random;

public class randomNumber {
	public static void main(String[] args) {
		Random rnd = new Random();
		
		int number = 0;
		
		for( int i =1; i<=10;i++) {
			System.out.println(number);
			number = rnd.nextInt(6);
			
			System.out.print(number + " " );
		}
	}

}
